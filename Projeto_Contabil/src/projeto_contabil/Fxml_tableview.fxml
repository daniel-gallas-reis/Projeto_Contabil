<GridPane alignment="CENTER" hgap="10.0" prefHeight="492.0" prefWidth="653.0" style="-fx-background-color: #1d1d1d;" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmltableview.FXMLTableViewController">
  <padding>
